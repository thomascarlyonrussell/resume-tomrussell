# Test environment configuration
# Copy this file to .env.test and fill in the values

# Base URL for tests (default: http://localhost:3000)
BASE_URL=http://localhost:3000

# Run tests in headless mode (default: true)
HEADLESS=true

# OpenRouter API key for chatbot tests (optional - some tests may fail without it)
OPENROUTER_API_KEY=your-api-key-here

# Chat model for testing (optional)
CHAT_MODEL=meta-llama/llama-3.3-70b-instruct:free
